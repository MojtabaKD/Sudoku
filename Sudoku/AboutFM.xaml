<Window x:Class="Sudoku.AboutFM"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sudoku"
        mc:Ignorable="d"
        Title="About" Height="450" Width="500" ResizeMode="NoResize">
    <Grid Loaded="Grid_Loaded">
        <Image Source="/Happy PePe.png" Width="250" Height="250" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <RichTextBox x:Name="MainRTB" FontSize="16" Width="300" HorizontalAlignment="Center" Margin="0,261,0,0" Height="140" VerticalAlignment="Top" IsDocumentEnabled="True" IsReadOnly="True"/>
    </Grid>
</Window>
